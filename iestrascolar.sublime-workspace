\";\n      $resul1=mysql_query($query1,$conexion);\n\n      if (mysql_fetch_row($resul1) == 0){\n        $queryInsert = \"INSERT INTO wp_terms(name,slug) VALUES ('$registro[subcategoria]','$registro[slug]')\";\n        $resultInsert = mysql_query($queryInsert,$conexion);\n        $idSubcategoria = \"SELECT term_id FROM wp_terms WHERE name = '$registro[subcategoria]'\";\n        $idSubcategoria = mysql_query($idSubcategoria, $conexion);\n        $idSubcategoria = mysql_fetch_array($idSubcategoria);\n        $idSubcategoria = (int)$idSubcategoria['term_id'];\n        // relacionar tabla wp_terms con tabla wp_term_taxonomy de la nueva subcategoria introducida\n        $query2 = \"SELECT wp_terms.term_id,subcategorias.description FROM wp_terms,subcategorias WHERE wp_terms.name LIKE '$registro[familia]' and subcategorias.subcategoria = '$registro[subcategoria]'\";\n        $resul2=mysql_query($query2,$conexion);\n\n        while($registro1=mysql_fetch_array($resul2)){\n          $queryInsert = \"INSERT INTO wp_term_taxonomy (term_id,taxonomy,description,parent) VALUES ($idSubcategoria,'product_cat','$registro1[description]',$registro1[term_id])\";\n          $resultInsert = mysql_query($queryInsert,$conexion);\n\n        }\n      }\n    }\n\n  // mysql_query($sql,$conexion);\n// echo \"Sale volcado de subcategorias<br/>\";\n\n  mysql_close($conexion);\n}\n\n///////////////////////\n///////PRODUCTOS///////\n///////////////////////\nif($prod != 0){\necho \"Comienza volcado de product<br/>\";\n\n  //volcado de la tabla productos a la tabla wp_posts\n    $conexion = mysql_connect($host,$user,$pass) OR DIE (mysql_error());\n    mysql_select_db($db,$conexion);\n  // mysqli_set_charset($conexion,\"utf8\");\n\n\n    $query = \"SELECT * FROM productos\";\n    $resulProd = mysql_query($query,$conexion);\n    while($registro=mysql_fetch_array($resulProd)){\n        $query1 = \"SELECT * FROM wp_posts WHERE wp_posts.id_producto = '$registro[id]'\";\n        $resul1=mysql_query($query1,$conexion);\n        if (mysql_num_rows($resul1) == 0){\n          // Añadir producto\n          $queryInsert = \"INSERT INTO wp_posts (post_author, post_date, post_content, post_title, post_status, comment_status, post_name, post_parent, menu_order, post_type, id_producto) VALUES(0,'$registro[post_date]','$registro[post_content]','$registro[post_title]', 'publish','$registro[comment_status]','$registro[post_name]','$registro[post_parent]','$registro[menu_order]','$registro[post_type]','$registro[id]')\";\n          $resultInsert = mysql_query($queryInsert,$conexion);\n\n          ///Para unir un producto con el tipo que es\n          $query2 = \"SELECT * FROM wp_terms where name = '$registro[product_type]' \";\n          $resul2=mysql_query($query2,$conexion);\n          $registro3 = mysql_fetch_row($resul2);\n          $codigoVariable = $registro3[0];//id del tipo\n\n          /////////////////////////////////// ETIQUETAS //////////////////////////////////////////\n          if($registro['slider']!=\"No\"){\n\n            /// Cojer la id de posts\n            $query60 = \"SELECT * FROM wp_posts where wp_posts.id_producto = '$registro[id]'\";\n\n            $resul60=mysql_query($query60,$conexion);\n            while($reg=mysql_fetch_array($resul60)){\n                $idProc = $reg['ID'];\n\n            }\n\n            /// Cojer la id de taxonomy\n            $query60 = \"SELECT t.* FROM wp_term_taxonomy t, wp_terms e WHERE e.name='\".$registro['slider'].\"' AND t.term_id=e.term_id\";\n\n            $resul60=mysql_query($query60,$conexion);\n            while($reg=mysql_fetch_array($resul60)){\n                $idTaxonomy = $reg['term_taxonomy_id'];\n\n            }\n\n            /// Añadir enlace de etiqueta con producto\n            if(!empty($idProc) && !empty($idTaxonomy)){\n                $queryInsert = \"INSERT INTO `wp_term_relationships`(`object_id`, `term_taxonomy_id`, `term_order`) VALUES (\".$idProc.\", \".$idTaxonomy.\", 0)\";\n                $re