$conexion,\"utf8\");\n\n\n            //echo \"entra cat<br>\";\n            $queryInsert = \"INSERT INTO categorias VALUES (null,'$linea[1]','$linea[2]','$linea[1]')\";\n            $resultInsert = mysql_query($queryInsert,$conexion);\n\n            mysql_close($conexion);\n    }\n    //echo \"entra cat<br>\";\n}\n\nfunction importarColores($direc,$file){\n\n\n    global $host,$user,$db,$pass;\n    $conexion = mysql_connect($host,$user,$pass) OR DIE (mysql_error());\n        mysql_select_db($db,$conexion);\n  // mysqli_set_charset($conexion,\"utf8\");\n\n    $fileOpen = fopen($direc. $file, \"r\");\n    while (($linea=fgetcsv($fileOpen,2048,\";\")) !== FALSE){\n\n            $queryInsert = \"INSERT INTO colores VALUES (null,'$linea[1]','$linea[2]','$linea[3]','$linea[2]')\";\n            $resultInsert = mysql_query($queryInsert,$conexion);\n\n\n        }\n        mysql_close($conexion);\n        //echo \"entra col<br>\";\n}\nfunction importarMarcas($direc,$file){\n\n    global $host,$user,$db,$pass;\n    $fileOpen = fopen($direc. $file, \"r\");\n\n    echo \"entra mar<br>\";\n\n    while (($linea=fgetcsv($fileOpen,2048,\";\")) !== FALSE){\n\n        $conexion = mysql_connect($host,$user,$pass) OR DIE (mysql_error());\n        mysql_select_db($db,$conexion);\n  // mysqli_set_charset($conexion,\"utf8\");\n\n\n            $queryInsert = \"INSERT INTO marcas VALUES (null,'$linea[1]','$linea[2]','$linea[1]')\";\n            // echo($queryInsert);\n            $resultInsert = mysql_query($queryInsert,$conexion);\n\n        mysql_close($conexion);\n\n    }\n}\nfunction importarProductos($direc,$file){\n\n    global $host,$user,$db,$pass;\n    $fileOpen = fopen($direc. $file, \"r\");\n    while (($linea=fgetcsv($fileOpen,2048,\";\")) !== FALSE){\n        $conexion = mysql_connect($host,$user,$pass) OR DIE (mysql_error());\n        mysql_select_db($db,$conexion);\n  // mysqli_set_charset($conexion,\"utf8\");\n\n        $linea[6] = date('Y-m-d H:i:s', strtotime($linea[6]));\n            $queryInsert = \"INSERT INTO productos VALUES ($linea[0],'$linea[1]','$linea[2]','$linea[3]',$linea[4],'$linea[5]','$linea[6]','$linea[7]','$linea[8]','$linea[9]','$linea[10]',$linea[11],'$linea[12]','$linea[13]','$linea[14]',$linea[15],$linea[16],'$linea[17]','$linea[18]','$linea[19]','$linea[20]','$linea[21]','$linea[22]','$linea[23]','$linea[24]','$linea[25]','$linea[26]','$linea[27]','No')\";\n            $resultInsert = mysql_query($queryInsert,$conexion);\n            mysql_close($conexion);\n    }\n\n    //echo \"entra pro<br>\";\n}\n\nfunction importarSubCategorias($direc,$file){\n\n    global $host,$user,$db,$pass;\n    $fileOpen = fopen($direc. $file, \"r\");\n    while (($linea=fgetcsv($fileOpen,2048,\";\")) !== FALSE){\n        $conexion = mysql_connect($host,$user,$pass) OR DIE (mysql_error());\n        mysql_select_db($db,$conexion);\n  // mysqli_set_charset($conexion,\"utf8\");\n\n\n            $queryInsert = \"INSERT INTO subcategorias VALUES (null,'$linea[1]','$linea[2]','$linea[3]','$linea[1]')\";\n            $resultInsert = mysql_query($queryInsert,$conexion);\n\n            mysql_close($conexion);\n    }\n}\nfunction importarTallas($direc,$file){\n\n    global $host,$user,$db,$pass;\n    $fileOpen = fopen($direc. $file, \"r\");\n    while (($linea=fgetcsv($fileOpen,2048,\";\")) !== FALSE){\n        $conexion = mysql_connect($host,$user,$pass) OR DIE (mysql_error());\n        mysql_select_db($db,$conexion);\n  // mysqli_set_charset($conexion,\"utf8\");\n\n\n            $queryInsert = \"INSERT INTO tallas VALUES (null,'$linea[1]','$linea[2]','$linea[3]','$linea[2]')\";\n            $resultInsert = mysql_query($queryInsert,$conexion);\n\n            mysql_close($conexion);\n\n    }\n    //echo \"entra tal<br>\";\n}\nfunction importarVariaciones($direc,$file){\n\n    global $host,$user,$db,$pass;\n    $conexion = mysql_connect($host,$user,$pass) OR DIE (mysql_error());\n    mysql_select_db($db,$conexion);\n  // mysqli_set_charset($conexion,\"utf8\");\n\n    $fileOpen = fopen($direc. $file, \"r\");\n    while (($linea=fgetcsv($fileOpen,2048,\";\")) !== FALSE){\n\n        $queryInsert = \"INSERT INTO variaciones VALUES (null,'$linea[0]','$linea[1]','$linea[2]','$linea[3]',$linea[4],$linea[5],$linea[6],'$linea[7]','$linea[8]')\";\n\n        // str_replace(\"/\", \"-\", $linea[8]).\"')\n        mysql_query($queryInsert,$conexion);\n    }\n    mysql_close($conexion);\n\n    //echo \"entra var<br>\";\n}\n\n\n////////////////////////\n///////CATEGORIAS//////\n///////////////////////\n  if($categ != 0){\n    // volcado de datos de la tabla categorias a la tabla wp_terms\n    $conexion = mysql_connect($host,$user,$pass) OR DIE (mysql_error());\n    mysql_select_db($db,$conexion);\n  // mysqli_set_charset($conexion,\"utf8\");\n    // echo \"Comienza volcado de categorias a wp post<br/>\";\n\n    $query = \"SELECT DISTINCT * FROM categorias\";\n    $resul=mysql_query($query,$conexion);\n\n    while($registro=mysql_fetch_array($resul)){\n      //Se busca si existen las categorias que hay en la tabla categorias, si existe no haca nada y si existe la inserta y le hace las relaciones\n\n      $query1 = \"SELECT * FROM wp_terms where name = '$registro[categoria]'\";\n      $resul1=mysql_query($query1,$conexion);\n\n      if (mysql_fetch_row($resul1) == 0){\n\n        $queryInsert = \"INSERT INTO wp_terms(name,slug) VALUES ('$registro[categoria]','$registro[slug]')\";\n        $resultInsert = mysql_query($queryInsert,$conexion);\n\n        // relacionar tabla wp_terms con tabla wp_term_taxonomy de la nueva categoria introducida\n        $query2 = \"SELECT wp_terms.term_id,categorias.description FROM wp_terms,categorias WHERE wp_terms.name = '$registro[categoria]' and categorias.categoria = '$registro[categoria]'\";\n        $resul2=mysql_query($query2,$conexion);\n\n\n        while($registro1=mysql_fetch_array($resul2)){\n\n          $queryInsert1 = \"INSERT INTO wp_term_taxonomy (term_id,taxonomy,description) VALUES ($registro1[term_id],'product_cat','$registro1[description]')\";\n          $resultInsert = mysql_query($queryInsert1,$conexion);\n        }\n      }\n    }\n\necho \"Sale de volcado de categorias<br/>\";\n    mysql_close($conexion);\n  }\n\n\n\n\n///////////////////////\n////////COLORES////////\n///////////////////////\n\nif($col != 0){\n// echo \"Comienza volcado de colores<br/>\";\n\n  // volcado de datos de la tabla colores a la tabla wp_terms\n  $conexion = mysql_connect($host,$user,$pass) OR DIE (mysql_error());\n  mysql_select_db($db,$conexion);\n  // mysqli_set_charset($conexion,\"utf8\");\n\n\n  $query = \"SELECT DISTINCT * FROM colores\";\n  $resul=mysql_query($query,$conexion);\n\n        while($registro=mysql_fetch_array($resul)){\n          //Se busca si existen las categorias que hay en la tabla colores, si existe no hace nada y si existe la inserta y le hace las relaciones\n\n          $query1 = \"SELECT * FROM wp_terms where name = '$registro[color]'\";\n          $resul1=mysql_query($query1,$conexion);\n\n          if (mysql_fetch_row($resul1) == 0){\n\n            $queryInsert = \"INSERT INTO wp_terms(name,slug) VALUES ('$registro[color]','$registro[slug]')\";\n            $resultInsert = mysql_query($queryInsert,$conexion);\n\n            // relacionar tabla wp_terms con tabla wp_term_taxonomy del nuevo color introducido\n            $query2 = \"SELECT wp_terms.term_id,colores.description FROM wp_terms,colores WHERE wp_terms.name='$registro[color]' and colores.color = '$registro[color]'\";\n            $resul2=mysql_query($query2,$conexion);\n\n\n            while($registro1=mysql_fetch_array($resul2)){\n\n              $queryInsert = \"INSERT INTO wp_term_taxonomy (term_id,taxonomy,description) VALUES ($registro1[term_id],'pa_color','$registro1[description]')\";\n              $resultInsert = mysql_query($queryInsert,$conexion);\n\n            }\n\n          }\n\n        }\n\necho \"Sale de volcado de colores<br/>\";\n  mysql_close($conexion)